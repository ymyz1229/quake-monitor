{
  "version": 2,
  "name": "quake-monitor",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/wolfx",
      "destination": "/api/wolfx.js"
    },
    {
      "source": "/api/usgs",
      "destination": "/api/usgs.js"
    }
  ]
}
